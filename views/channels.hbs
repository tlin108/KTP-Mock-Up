<div class="container">
  <div class="row">
      <div class="col-lg-10 col-lg-offset-1">
      {{#each channel}}
        <article class="list-group">
          <a href="#" class="list-group-item">
            <div class="row">
              <div class="col-lg-1">
                <figure>
                  <img src={{ this.subjectPhotoUrl }} alt="Subject Photo" width="60" height="60">
                </figure>
              </div>
              <div class="col-lg-5">
                <h4 class="list-group-item-heading">{{ this.title }}</h4>
                <p class="list-group-item-text">{{ this.description }}</p>
              </div>
              <div class="col-lg-1">
                <figure>
                  <img src={{ this.instructorPhotoUrl }} alt="Instructor Photo" width="60" height="60">
                </figure>
              </div>
              <div class="col-lg-2">
                <p class="list-group-item-text">{{ this.instructorName }}</p>
              </div>
              <div class="col-lg-3">
                <p class="list-group-item-text">{{ this.period }}</p>
              </div>
            </div>
          </a>
        </article>
      {{/each}}
      </div>
  </div>
</div>